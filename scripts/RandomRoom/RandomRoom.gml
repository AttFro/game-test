function RandomRoom(){
randomNumber = irandom_range(0, 4);
if (randomNumber = 0)
{
	oPlayer.RanRoom = Room1
}
if (randomNumber = 1)
{
	oPlayer.RanRoom = Room2
}
if (randomNumber = 2)
{
	oPlayer.RanRoom = Room3
}
if (randomNumber = 3)
{
	oPlayer.RanRoom = Room4
}
if (randomNumber = 4)
{
	oPlayer.RanRoom = Room5
}
}